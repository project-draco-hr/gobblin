{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  KafkaRecord other=(KafkaRecord)obj;
  if (key == null) {
    if (other.key != null) {
      return false;
    }
  }
 else   if (!key.equals(other.key)) {
    return false;
  }
  if (offset != other.offset) {
    return false;
  }
  if (payload == null) {
    if (other.payload != null) {
      return false;
    }
  }
 else   if (!payload.equals(other.payload)) {
    return false;
  }
  return true;
}
