{
  updateTaskMetrics();
  for (  Optional<Fork> fork : this.task.getForks()) {
    if (fork.isPresent() && fork.get().queueStats().isPresent()) {
      logger.info(String.format("Queue stats of fork %d of task %s: %s",fork.get().getIndex(),this.task.getTaskId(),fork.get().queueStats().get().toString()));
    }
  }
}
