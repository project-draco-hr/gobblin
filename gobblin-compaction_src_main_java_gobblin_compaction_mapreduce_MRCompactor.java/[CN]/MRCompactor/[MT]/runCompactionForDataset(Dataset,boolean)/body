{
  LOG.info("Running compaction for dataset " + dataset);
  try {
    MRCompactorJobRunner jobRunner=getMRCompactorJobRunner(dataset,dataset.priority());
    this.jobRunnables.put(dataset,jobRunner);
    if (proceed) {
      jobRunner.proceed();
    }
    this.jobExecutor.execute(jobRunner);
  }
 catch (  Throwable t) {
    dataset.skip(t);
  }
}
