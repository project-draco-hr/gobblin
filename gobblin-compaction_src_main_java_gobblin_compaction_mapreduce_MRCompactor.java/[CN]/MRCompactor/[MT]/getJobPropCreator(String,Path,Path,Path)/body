{
  String builderClassName=this.state.getProp(COMPACTION_JOBPROPS_CREATOR_CLASS,DEFAULT_COMPACTION_JOBPROPS_CREATOR_CLASS) + "$Builder";
  boolean deduplicate=this.state.getPropAsBoolean(COMPACTION_DEDUPLICATE,DEFAULT_COMPACTION_DEDUPLICATE);
  try {
    return ((MRCompactorJobPropCreator.Builder<?>)Class.forName(builderClassName).newInstance()).withTopic(topic).withTopicInputDir(topicInputDir).withTopicOutputDir(topicOutputDir).withTopicTmpDir(topicTmpDir).withFileSystem(this.fs).withDeduplicate(deduplicate).withState(this.state).build();
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
