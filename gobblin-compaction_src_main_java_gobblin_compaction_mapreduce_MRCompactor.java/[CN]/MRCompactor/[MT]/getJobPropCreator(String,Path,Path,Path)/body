{
  String builderClassName=this.state.getProp(ConfigurationKeys.COMPACTION_JOBPROPS_CREATOR_CLASS,ConfigurationKeys.DEFAULT_COMPACTION_JOBPROPS_CREATOR_CLASS) + "$Builder";
  try {
    return ((MRCompactorJobPropCreator.Builder<?>)Class.forName(builderClassName).newInstance()).withTopic(topic).withTopicInputDir(topicInputDir).withTopicOutputDir(topicOutputDir).withTopicTmpDir(topicTmpDir).withFileSystem(this.fs).withState(this.state).build();
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
