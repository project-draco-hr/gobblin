{
  try {
    copyDependencyJarsToHdfs();
    processDatasets();
    throwExceptionsIfAnyDatasetCompactionFailed();
  }
 catch (  Throwable t) {
    LOG.error("Caught throwable during compaction",t);
    throw Throwables.propagate(t);
  }
 finally {
    try {
      shutdownExecutors();
      this.closer.close();
    }
  finally {
      deleteDependencyJars();
      gobblinMetrics.stopMetricReporting();
    }
  }
}
