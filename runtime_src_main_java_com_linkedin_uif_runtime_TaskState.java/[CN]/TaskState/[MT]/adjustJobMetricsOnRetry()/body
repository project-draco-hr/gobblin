{
  Metrics metrics=Metrics.get(this.getProp(ConfigurationKeys.JOB_NAME_KEY),this.jobId);
  long recordsWritten=metrics.getCounter(Metrics.metricName(Metrics.MetricGroup.TASK,this.taskId,"records")).getCount();
  long bytesWritten=metrics.getCounter(Metrics.metricName(Metrics.MetricGroup.TASK,this.taskId,"bytes")).getCount();
  metrics.getCounter(Metrics.metricName(Metrics.MetricGroup.JOB,this.jobId,"records")).dec(recordsWritten);
  metrics.getCounter(Metrics.metricName(Metrics.MetricGroup.JOB,this.jobId,"bytes")).dec(bytesWritten);
}
