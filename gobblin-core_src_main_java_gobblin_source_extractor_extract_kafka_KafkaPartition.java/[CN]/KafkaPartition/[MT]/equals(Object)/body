{
  if (!(obj instanceof KafkaPartition)) {
    return false;
  }
  KafkaPartition other=(KafkaPartition)obj;
  if (id != other.id) {
    return false;
  }
  if (topicName == null) {
    if (other.topicName != null) {
      return false;
    }
  }
 else   if (!topicName.equals(other.topicName)) {
    return false;
  }
  return true;
}
