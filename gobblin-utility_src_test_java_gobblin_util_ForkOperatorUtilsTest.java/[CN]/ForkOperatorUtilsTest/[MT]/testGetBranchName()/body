{
  State state=new State();
  state.setProp(ConfigurationKeys.FORK_BRANCH_NAME_KEY + ".0",FORK_BRANCH_NAME_0);
  state.setProp(ConfigurationKeys.FORK_BRANCH_NAME_KEY + ".1",FORK_BRANCH_NAME_1);
  Assert.assertEquals(ForkOperatorUtils.getBranchName(state,0,ConfigurationKeys.DEFAULT_FORK_BRANCH_NAME + 0),FORK_BRANCH_NAME_0);
  Assert.assertEquals(ForkOperatorUtils.getBranchName(state,1,ConfigurationKeys.DEFAULT_FORK_BRANCH_NAME + 1),FORK_BRANCH_NAME_1);
  Assert.assertEquals(ForkOperatorUtils.getBranchName(state,2,ConfigurationKeys.DEFAULT_FORK_BRANCH_NAME + 2),ConfigurationKeys.DEFAULT_FORK_BRANCH_NAME + 2);
}
