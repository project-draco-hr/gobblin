{
  while (!this.stopped) {
    try {
      WorkUnitState workUnitState=this.workUnitQueue.poll(5,TimeUnit.SECONDS);
      if (workUnitState == null) {
        continue;
      }
      Task task=new Task(new TaskContext(workUnitState),this.taskStateTracker,taskExecutor,Optional.<CountDownLatch>absent());
      this.taskExecutor.execute(task);
    }
 catch (    InterruptedException ie) {
    }
  }
}
