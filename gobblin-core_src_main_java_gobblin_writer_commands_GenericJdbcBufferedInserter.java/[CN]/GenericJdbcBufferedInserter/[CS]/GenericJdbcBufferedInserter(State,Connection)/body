{
  this.conn=conn;
  this.maxBatchSize=state.getPropAsInt(WRITER_JDBC_INSERT_BATCH_SIZE,DEFAULT_WRITER_JDBC_INSERT_BATCH_SIZE);
  this.maxParamSize=state.getPropAsInt(WRITER_JDBC_MAX_PARAM_SIZE,DEFAULT_WRITER_JDBC_MAX_PARAM_SIZE);
  this.retryer=RetryerBuilder.<Void>newBuilder().retryIfException().withWaitStrategy(WaitStrategies.exponentialWait(1000,30,TimeUnit.SECONDS)).withStopStrategy(StopStrategies.stopAfterAttempt(5)).build();
}
