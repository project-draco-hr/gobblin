{
  if (this.pStmt == null) {
    initializeBatch(databaseName,table,jdbcEntryData);
  }
  int i=0;
  for (  JdbcEntryDatum datum : jdbcEntryData) {
    this.pStmt.setObject(++i,datum.getVal());
  }
  this.pStmt.addBatch();
  this.currBatchSize++;
  if (this.currBatchSize >= this.maxBatchSize) {
    insertBatch();
  }
}
