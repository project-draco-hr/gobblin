{
  this.executor=Executors.newFixedThreadPool(10);
  this.context=MetricContext.builder("Job").addTag(new Tag<String>(JOB_NAME_KEY,"ExampleJob")).addTag(new Tag<String>(JOB_ID_KEY,JOB_NAME + "_" + System.currentTimeMillis())).build();
  this.reporterBuilder=reporterBuilder;
  this.tasks=tasks;
  this.totalRecords=totalRecords;
}
