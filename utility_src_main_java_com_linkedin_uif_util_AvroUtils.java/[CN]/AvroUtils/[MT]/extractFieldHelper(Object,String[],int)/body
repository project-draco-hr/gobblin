{
  if ((field + 1) == fieldPath.length) {
    Object result=((Record)data).get(fieldPath[field]);
    if (result == null) {
      return Optional.absent();
    }
 else {
      return Optional.of(result);
    }
  }
 else {
    return extractFieldHelper(((Record)data).get(fieldPath[field]),fieldPath,++field);
  }
}
