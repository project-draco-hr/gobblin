{
  this.context.progress();
  if (Metrics.isEnabled(this.task.getTaskState().getWorkunit())) {
    this.task.updateRecordMetrics();
    String taskRecordMetric=Metrics.metricName(Metrics.MetricGroup.TASK,task.getTaskId(),"records");
    this.context.getCounter(Metrics.MetricGroup.TASK.name(),taskRecordMetric).setValue(Metrics.get(this.task.getTaskState().getProp(ConfigurationKeys.JOB_NAME_KEY),this.task.getJobId()).getCounter(taskRecordMetric).getCount());
  }
}
