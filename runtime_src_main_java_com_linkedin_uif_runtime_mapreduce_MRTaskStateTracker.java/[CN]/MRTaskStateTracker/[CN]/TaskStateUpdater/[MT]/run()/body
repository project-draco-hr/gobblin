{
  this.context.progress();
  if (Metrics.isEnabled(this.task.getTaskState().getWorkunit())) {
    this.task.updateRecordMetrics();
    String taskRecordMetric=Metrics.metricName(TaskState.TASK_METRICS_PREFIX,task.getTaskId(),"records");
    this.context.getCounter(TaskState.TASK_METRICS_PREFIX,taskRecordMetric).setValue(Metrics.get(this.task.getTaskState().getProp(ConfigurationKeys.JOB_NAME_KEY),this.task.getJobId()).getCounter(taskRecordMetric).getCount());
  }
}
