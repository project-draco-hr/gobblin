{
  if (sftp == null) {
    sftp=(ChannelSftp)SftpExecutor.connect(this.workUnit.getProp(ConfigurationKeys.SOURCE_PRIVATE_KEY),this.workUnit.getProp(ConfigurationKeys.SOURCE_KNOWN_HOSTS),this.workUnit.getProp(ConfigurationKeys.SOURCE_USERNAME),this.workUnit.getProp(ConfigurationKeys.SOURCE_HOST_NAME));
  }
  return this.sftp;
}
