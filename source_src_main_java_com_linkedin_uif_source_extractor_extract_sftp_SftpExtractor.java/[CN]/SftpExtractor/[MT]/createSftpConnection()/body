{
  ChannelSftp sftp=(ChannelSftp)SftpHelper.connect(this.workUnitState.getProp(ConfigurationKeys.SOURCE_CONN_PRIVATE_KEY),this.workUnitState.getProp(ConfigurationKeys.SOURCE_CONN_KNOWN_HOSTS),this.workUnitState.getProp(ConfigurationKeys.SOURCE_CONN_USERNAME),this.workUnitState.getProp(ConfigurationKeys.SOURCE_CONN_HOST_NAME),this.workUnitState.getPropAsInt(ConfigurationKeys.SOURCE_CONN_PORT,ConfigurationKeys.SOURCE_CONN_DEFAULT_PORT),this.workUnitState.getProp(ConfigurationKeys.SOURCE_CONN_USE_PROXY_URL),this.workUnitState.getPropAsInt(ConfigurationKeys.SOURCE_CONN_USE_PROXY_PORT,-1));
  return sftp;
}
