{
  this.session=SftpExecutor.connect(this.workUnitState.getProp(ConfigurationKeys.SOURCE_CONN_PRIVATE_KEY),this.workUnitState.getProp(ConfigurationKeys.SOURCE_CONN_KNOWN_HOSTS),this.workUnitState.getProp(ConfigurationKeys.SOURCE_CONN_USERNAME),this.workUnitState.getProp(ConfigurationKeys.SOURCE_CONN_HOST_NAME),this.workUnitState.getProp(ConfigurationKeys.SOURCE_CONN_USE_PROXY_URL),this.workUnitState.getPropAsInt(ConfigurationKeys.SOURCE_CONN_USE_PROXY_PORT,-1));
  try {
    log.info("Establishing a SFTP channel");
    this.channelSftp=(ChannelSftp)this.session.openChannel("sftp");
    this.channelSftp.connect();
  }
 catch (  JSchException je) {
    log.error("Failed to establish a SFTP channel",je);
    throw new RuntimeException(je);
  }
}
