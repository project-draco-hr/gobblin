{
  return LogCopier.newBuilder().useSrcFileSystem(FileSystem.getLocal(new Configuration())).useDestFileSystem(destFs).readFrom(getLocalLogDir()).writeTo(getHdfsLogDir(destFs,appWorkDir)).acceptsLogFileExtensions(ImmutableSet.of(ApplicationConstants.STDOUT,ApplicationConstants.STDERR)).useLogFileNamePrefix(containerId.toString()).build();
}
