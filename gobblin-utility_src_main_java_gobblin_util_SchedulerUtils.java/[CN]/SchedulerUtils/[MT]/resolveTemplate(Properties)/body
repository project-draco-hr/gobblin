{
  if (jobProps.containsKey(ConfigurationKeys.JOB_TEMPLATE_PATH)) {
    Properties resolvedProps=ConfigUtils.configToProperties((new ResourceBasedTemplate(jobProps.getProperty(ConfigurationKeys.JOB_TEMPLATE_PATH))).getResolvedConfig(ConfigFactory.parseProperties(jobProps)));
    return resolvedProps;
  }
 else {
    return jobProps;
  }
}
