  jobProps.setProperty(ConfigurationKeys.MR_JOB_MAX_MAPPERS_KEY,"3");
  runTest(jobProps);
  jobProps.setProperty(ConfigurationKeys.MR_JOB_MAX_MAPPERS_KEY,"5");
  runTest(jobProps);
}
