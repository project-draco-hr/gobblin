{
  try {
switch (state) {
case CONNECTING:
      connect();
    break;
case WRITING:
  write();
break;
case READING:
read();
break;
default :
throw new IllegalStateException("ProxySetupHandler should not be in state " + state);
}
}
 catch (IOException ioe) {
LOG.warn("Failed to establish a proxy connection for {}",client.getRemoteAddress(),ioe);
closeChannels();
}
return state;
}
