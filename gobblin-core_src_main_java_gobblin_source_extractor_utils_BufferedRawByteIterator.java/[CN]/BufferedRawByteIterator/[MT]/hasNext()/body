{
  if (this.nextElement.isPresent()) {
    return true;
  }
  try {
    int bytes=this.is.read(this.buffer,0,this.bufferSize);
    if (bytes <= 0) {
      return false;
    }
    this.nextElement=Optional.of(Arrays.copyOf(this.buffer,bytes));
    return true;
  }
 catch (  IOException ioe) {
    return false;
  }
}
