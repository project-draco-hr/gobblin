{
  List<URI> result=new ArrayList<URI>();
  if (relativeURI == null || relativeURI.size() == 0)   return result;
  for (  URI tmp : relativeURI) {
    result.add(new URI(storeRootURI.getScheme(),storeRootURI.getAuthority(),storeRootURI.getPath() + "/" + tmp.getPath(),storeRootURI.getQuery(),storeRootURI.getFragment()));
  }
  return result;
}
