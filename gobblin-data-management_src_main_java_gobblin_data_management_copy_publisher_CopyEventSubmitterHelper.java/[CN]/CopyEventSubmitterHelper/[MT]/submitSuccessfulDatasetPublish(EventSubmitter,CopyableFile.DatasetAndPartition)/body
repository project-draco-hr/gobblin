{
  SlaEventSubmitter.builder().eventSubmitter(eventSubmitter).eventName(DATASET_PUBLISHED_EVENT_NAME).datasetUrn(datasetAndPartition.getDataset().getDatasetRoot().toString()).partition(datasetAndPartition.getPartition()).additionalMetadata(DATASET_TARGET_ROOT_METADATA_NAME,datasetAndPartition.getDataset().getDatasetTargetRoot().toString()).build().submit();
}
