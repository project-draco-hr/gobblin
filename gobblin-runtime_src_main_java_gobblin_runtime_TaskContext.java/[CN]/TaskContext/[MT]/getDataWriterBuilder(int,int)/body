{
  String dataWriterBuilderClassName=this.workUnit.getProp(ForkOperatorUtils.getPropertyNameForBranch(ConfigurationKeys.WRITER_BUILDER_CLASS,branches,index),ConfigurationKeys.DEFAULT_WRITER_BUILDER_CLASS);
  try {
    return DataWriterBuilder.class.cast(Class.forName(dataWriterBuilderClassName).newInstance());
  }
 catch (  ClassNotFoundException cnfe) {
    throw new RuntimeException(cnfe);
  }
catch (  InstantiationException ie) {
    throw new RuntimeException(ie);
  }
catch (  IllegalAccessException iae) {
    throw new RuntimeException(iae);
  }
}
