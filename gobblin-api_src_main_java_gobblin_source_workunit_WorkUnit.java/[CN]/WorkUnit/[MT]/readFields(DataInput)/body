{
  super.readFields(in);
  this.extract.readFields(in);
  Closer closer=Closer.create();
  try {
    ByteArrayInputStream watermarkIntervalIn=closer.register(new ByteArrayInputStream(getProp(ConfigurationKeys.WATERMARK_INTERVAL_VALUE_KEY).getBytes()));
    this.watermarkInterval=new WatermarkInterval();
    this.watermarkInterval.readFields(closer.register(new DataInputStream(watermarkIntervalIn)));
  }
  finally {
    closer.close();
  }
}
