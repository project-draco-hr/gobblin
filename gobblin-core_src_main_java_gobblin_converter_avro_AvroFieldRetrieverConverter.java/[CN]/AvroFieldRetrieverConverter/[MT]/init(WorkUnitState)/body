{
  Preconditions.checkArgument(workUnit.contains(ConfigurationKeys.CONVERTER_AVRO_EXTRACTOR_FIELD_PATH),"The converter " + this.getClass().getName() + " cannot be used without setting the property "+ ConfigurationKeys.CONVERTER_AVRO_EXTRACTOR_FIELD_PATH);
  this.fieldLocation=workUnit.getProp(ConfigurationKeys.CONVERTER_AVRO_EXTRACTOR_FIELD_PATH);
  return this;
}
