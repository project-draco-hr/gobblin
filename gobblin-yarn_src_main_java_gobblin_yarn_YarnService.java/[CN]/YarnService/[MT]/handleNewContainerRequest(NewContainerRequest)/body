{
  int newContainersRequested=newContainerRequest.getNewContainersRequested();
  if (newContainersRequested <= 0) {
    LOGGER.error("Number of containers requested me be positive");
    return;
  }
  if (!this.maxResourceCapacity.isPresent()) {
    LOGGER.error(String.format("Unable to handle new container request as maximum resource capacity is not available: " + "[containers requested = %d, memory (MBs) requested = %d, vcores requested = %d]",newContainersRequested,this.requestedContainerMemoryMbs,this.requestedContainerCores));
    return;
  }
  try {
    requestContainers(newContainersRequested);
  }
 catch (  IOException ioe) {
    LOGGER.error("Failed to handle new container request",ioe);
  }
catch (  YarnException ye) {
    LOGGER.error("Failed to handle new container request",ye);
  }
}
