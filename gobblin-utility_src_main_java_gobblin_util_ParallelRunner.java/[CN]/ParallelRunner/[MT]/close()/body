{
  try {
    for (    Future<?> future : this.futures) {
      future.get();
    }
  }
 catch (  InterruptedException ie) {
    throw new IOException(ie);
  }
catch (  ExecutionException ee) {
    throw new IOException(ee);
  }
 finally {
    ExecutorsUtils.shutdownExecutorService(this.executor);
  }
}
