{
  try {
    ExecutorsUtils.shutdownExecutorService(this.applicationStatusMonitor);
    sendShutdownRequest();
    this.yarnClient.stop();
    this.helixManager.disconnect();
  }
  finally {
    try {
      if (this.applicationId != null) {
        cleanUpAppWorkDirectory(this.applicationId);
      }
    }
  finally {
      this.fs.close();
    }
  }
}
