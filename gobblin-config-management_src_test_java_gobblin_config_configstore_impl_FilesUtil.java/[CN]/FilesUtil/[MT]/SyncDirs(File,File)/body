{
  if (!input.exists())   return;
  if (output.exists())   return;
  output.mkdirs();
  if (input.isFile() || output.isFile())   return;
  File[] children=input.listFiles();
  File tmp;
  for (  File f : children) {
    tmp=new File(output,f.getName());
    if (f.isDirectory()) {
      SyncDirs(f,tmp);
    }
 else {
      Files.copy(f,tmp);
    }
  }
}
