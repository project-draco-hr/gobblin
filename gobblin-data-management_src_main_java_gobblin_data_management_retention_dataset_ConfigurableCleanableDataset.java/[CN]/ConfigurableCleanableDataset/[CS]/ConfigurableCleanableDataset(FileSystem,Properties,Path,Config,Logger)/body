{
  super(fs,jobProps,log);
  this.datasetRoot=datasetRoot;
  Preconditions.checkArgument(config.hasPath(VERSION_FINDER_CLASS_KEY),"Missing property " + VERSION_FINDER_CLASS_KEY);
  Preconditions.checkArgument(config.hasPath(RETENTION_POLICY_CLASS_KEY),"Missing property " + RETENTION_POLICY_CLASS_KEY);
  try {
    this.versionFinder=(VersionFinder<? extends T>)ConstructorUtils.invokeConstructor(Class.forName(config.getString(VERSION_FINDER_CLASS_KEY)),this.fs,config);
    this.retentionPolicy=(RetentionPolicy<T>)ConstructorUtils.invokeConstructor(Class.forName(config.getString(RETENTION_POLICY_CLASS_KEY)),config);
  }
 catch (  NoSuchMethodException|IllegalAccessException|InvocationTargetException|InstantiationException|ClassNotFoundException e) {
    throw new IllegalArgumentException(e);
  }
}
