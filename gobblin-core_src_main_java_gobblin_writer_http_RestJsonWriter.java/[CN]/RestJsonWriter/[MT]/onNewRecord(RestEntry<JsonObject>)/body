{
  HttpUriRequest uriRequest=RequestBuilder.post().addHeader(HttpHeaders.CONTENT_TYPE,ContentType.APPLICATION_JSON.getMimeType()).setUri(combineUrl(getCurServerHost(),record.getResourcePath())).setEntity(new StringEntity(record.getRestEntryVal().toString(),ContentType.APPLICATION_JSON.toString())).build();
  return Optional.of(uriRequest);
}
