{
  this(conf.getInt(ConfigurationKeys.TASK_EXECUTOR_THREADPOOL_SIZE_KEY,ConfigurationKeys.DEFAULT_TASK_EXECUTOR_THREADPOOL_SIZE),conf.getInt(ConfigurationKeys.TASK_RETRY_THREAD_POOL_CORE_SIZE_KEY,ConfigurationKeys.DEFAULT_TASK_RETRY_THREAD_POOL_CORE_SIZE),conf.getLong(ConfigurationKeys.TASK_RETRY_INTERVAL_IN_SEC_KEY,ConfigurationKeys.DEFAULT_TASK_RETRY_INTERVAL_IN_SEC));
}
