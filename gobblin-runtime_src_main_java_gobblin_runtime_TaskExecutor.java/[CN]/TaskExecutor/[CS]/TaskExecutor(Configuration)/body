{
  this(Integer.parseInt(conf.get(ConfigurationKeys.TASK_EXECUTOR_THREADPOOL_SIZE_KEY,ConfigurationKeys.DEFAULT_TASK_EXECUTOR_THREADPOOL_SIZE)),Integer.parseInt(conf.get(ConfigurationKeys.TASK_RETRY_THREAD_POOL_CORE_SIZE_KEY,ConfigurationKeys.DEFAULT_TASK_RETRY_THREAD_POOL_CORE_SIZE)),Integer.parseInt(conf.get(ConfigurationKeys.TASK_RETRY_THREAD_POOL_MAX_SIZE_KEY,ConfigurationKeys.DEFAULT_TASK_RETRY_THREAD_POOL_MAX_SIZE)),Long.parseLong(conf.get(ConfigurationKeys.TASK_RETRY_INTERVAL_IN_SEC_KEY,ConfigurationKeys.DEFAULT_TASK_RETRY_INTERVAL_IN_SEC)));
}
