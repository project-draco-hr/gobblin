{
  HadoopFsHelper hfsHelper=(HadoopFsHelper)this.fsHelper;
  if (this.filesToPull.isEmpty()) {
    return null;
  }
 else {
    try {
      return hfsHelper.getAvroSchema(this.filesToPull.get(0));
    }
 catch (    FileBasedHelperException e) {
      Throwables.propagate(e);
      return null;
    }
  }
}
