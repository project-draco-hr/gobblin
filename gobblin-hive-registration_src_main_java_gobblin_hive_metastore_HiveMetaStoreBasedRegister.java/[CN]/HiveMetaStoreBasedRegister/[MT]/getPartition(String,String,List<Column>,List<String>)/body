{
  try (AutoReturnableObject<IMetaStoreClient> client=this.clientPool.getClient();HiveLock lock=locks.getTableLock(dbName,tableName)){
    return Optional.of(HiveMetaStoreUtils.getHivePartition(client.get().getPartition(tableName,dbName,partitionValues)));
  }
 catch (  NoSuchObjectException e) {
    return Optional.<HivePartition>absent();
  }
catch (  TException e) {
    throw new IOException("Unable to get partition " + partitionValues + " from table "+ tableName+ " in db "+ dbName,e);
  }
}
