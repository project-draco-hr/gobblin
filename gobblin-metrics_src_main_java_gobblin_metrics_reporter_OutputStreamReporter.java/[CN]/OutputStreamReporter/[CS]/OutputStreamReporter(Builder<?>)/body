{
  super(builder.registry,"console-reporter",builder.filter,builder.rateUnit,builder.durationUnit);
  this.closer=Closer.create();
  this.output=closer.register(builder.output);
  this.locale=builder.locale;
  this.clock=builder.clock;
  this.dateFormat=DateFormat.getDateTimeInstance(DateFormat.SHORT,DateFormat.MEDIUM,locale);
  this.tags=builder.tags;
  this.dateFormat.setTimeZone(builder.timeZone);
  this.outputBuffer=new ByteArrayOutputStream();
  this.outputBufferPrintStream=this.closer.register(new PrintStream(this.outputBuffer));
}
