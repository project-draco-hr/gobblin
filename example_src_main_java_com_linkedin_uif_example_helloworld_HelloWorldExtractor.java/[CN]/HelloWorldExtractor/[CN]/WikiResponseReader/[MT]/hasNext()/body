{
  if (numRecordsRead < recordsOfCurrentTitle.size()) {
    return true;
  }
 else   if (currentTitleId >= requestedTitles.size() - 1) {
    return false;
  }
 else {
    while (currentTitleId < requestedTitles.size() - 1) {
      currentTitleId++;
      try {
        recordsOfCurrentTitle=retrievePageRevisions(HelloWorldExtractor.this.requestedTitles.get(currentTitleId));
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
      if (!recordsOfCurrentTitle.isEmpty()) {
        numRecordsRead=0;
        return true;
      }
    }
    return false;
  }
}
