{
  if (classA.isAssignableFrom(classB)) {
    return classA;
  }
 else {
    Class<?> klazz=classA;
    while (!klazz.isAssignableFrom(classB)) {
      klazz=klazz.getSuperclass();
    }
    if (DatasetVersion.class.isAssignableFrom(klazz)) {
      return (Class<T>)klazz;
    }
 else {
      return (Class<T>)DatasetVersion.class;
    }
  }
}
