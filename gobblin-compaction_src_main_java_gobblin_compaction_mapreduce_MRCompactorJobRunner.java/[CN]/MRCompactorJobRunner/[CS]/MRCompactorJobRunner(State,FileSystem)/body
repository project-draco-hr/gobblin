{
  this.jobProps=jobProps;
  this.topic=jobProps.getProp(ConfigurationKeys.COMPACTION_TOPIC);
  this.inputPath=new Path(jobProps.getProp(ConfigurationKeys.COMPACTION_JOB_INPUT_DIR));
  this.outputPath=new Path(jobProps.getProp(ConfigurationKeys.COMPACTION_JOB_DEST_DIR));
  this.tmpPath=new Path(jobProps.getProp(ConfigurationKeys.COMPACTION_JOB_TMP_DIR));
  this.fs=fs;
  this.perm=HadoopUtils.deserializeFsPermission(this.jobProps,ConfigurationKeys.COMPACTION_OUTPUT_DIR_PERMISSION,FsPermission.getDefault());
  this.deduplicate=this.jobProps.getPropAsBoolean(ConfigurationKeys.COMPACTION_DEDUPLICATE,ConfigurationKeys.DEFAULT_COMPACTION_DEDUPLICATE);
  this.eventSubmitter=new EventSubmitter.Builder(GobblinMetrics.get(this.jobProps.getProp(ConfigurationKeys.JOB_NAME_KEY)).getMetricContext(),ConfigurationKeys.COMPACTION_TRACKING_EVENTS_NAMESPACE).build();
}
