{
  if (this.converterForWriter == null) {
    String converterClassesList=this.workUnit.getProp(ConfigurationKeys.CONVERTER_CLASSES_KEY);
    String converterClassForWriter=Lists.newLinkedList(Splitter.on(",").omitEmptyStrings().trimResults().split(converterClassesList)).getLast();
    try {
      this.converterForWriter=(Converter)Class.forName(converterClassForWriter).newInstance();
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
  this.converterForWriter.init(workUnitState);
  return this.converterForWriter;
}
