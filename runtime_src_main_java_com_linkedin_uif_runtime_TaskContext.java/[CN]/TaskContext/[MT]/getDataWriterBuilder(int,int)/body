{
  try {
    return (DataWriterBuilder)Class.forName(this.workUnit.getProp(ConfigurationKeys.WRITER_BUILDER_CLASS + (branches > 1 ? "." + index : ""),ConfigurationKeys.DEFAULT_WRITER_BUILDER_CLASS)).newInstance();
  }
 catch (  Exception e) {
    throw new IOException("Failed to instantiate a DataWriterBuilder",e);
  }
}
