{
  delete();
  Closer closer=Closer.create();
  try {
    if (!this.fs.exists(this.applicationIdFilePath.getParent())) {
      this.fs.mkdirs(this.applicationIdFilePath.getParent());
    }
    CharStreams.asWriter(closer.register(new OutputStreamWriter(this.fs.create(this.applicationIdFilePath)))).write(applicationId);
  }
 catch (  Throwable t) {
    throw closer.rethrow(t);
  }
 finally {
    closer.close();
  }
}
