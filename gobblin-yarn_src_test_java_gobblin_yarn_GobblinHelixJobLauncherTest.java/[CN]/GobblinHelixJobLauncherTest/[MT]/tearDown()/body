{
  try {
    this.gobblinWorkUnitRunner.stop();
    this.thread.join();
  }
 catch (  InterruptedException ie) {
    Thread.currentThread().interrupt();
  }
  try {
    this.helixManager.disconnect();
    if (this.localFs.exists(this.appWorkDir)) {
      this.localFs.delete(this.appWorkDir,true);
    }
  }
  finally {
    this.closer.close();
  }
}
