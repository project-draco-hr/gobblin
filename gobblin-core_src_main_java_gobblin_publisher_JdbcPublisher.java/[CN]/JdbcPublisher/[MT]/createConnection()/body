{
  DataSource dataSource=DataSourceBuilder.builder().url(state.getProp(JDBC_PUBLISHER_URL)).driver(state.getProp(JDBC_PUBLISHER_DRIVER)).userName(state.getProp(JDBC_PUBLISHER_USERNAME)).passWord(state.getProp(JDBC_PUBLISHER_PASSWORD)).cryptoKeyLocation(state.getProp(JDBC_PUBLISHER_ENCRYPTION_KEY_LOC)).maxActiveConnections(1).maxIdleConnections(1).state(state).build();
  try {
    return dataSource.getConnection();
  }
 catch (  SQLException e) {
    throw new RuntimeException(e);
  }
}
