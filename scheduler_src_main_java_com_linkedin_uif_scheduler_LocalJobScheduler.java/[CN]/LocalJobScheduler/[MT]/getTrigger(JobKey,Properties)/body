{
  Trigger trigger=TriggerBuilder.newTrigger().withIdentity(Strings.nullToEmpty(properties.getProperty(ConfigurationKeys.JOB_NAME_KEY)),Strings.nullToEmpty(properties.getProperty(ConfigurationKeys.JOB_GROUP_KEY))).forJob(jobKey).withSchedule(CronScheduleBuilder.cronSchedule(properties.getProperty(ConfigurationKeys.JOB_SCHEDULE_KEY))).build();
  return trigger;
}
