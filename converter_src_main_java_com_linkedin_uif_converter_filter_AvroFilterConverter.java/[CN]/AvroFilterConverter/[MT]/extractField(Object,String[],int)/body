{
  if ((field + 1) == fieldPath.length) {
    String result=String.valueOf(((Record)data).get(fieldPath[field]));
    if (result == null) {
      return null;
    }
 else {
      return result;
    }
  }
 else {
    return extractField(((Record)data).get(fieldPath[field]),fieldPath,++field);
  }
}
