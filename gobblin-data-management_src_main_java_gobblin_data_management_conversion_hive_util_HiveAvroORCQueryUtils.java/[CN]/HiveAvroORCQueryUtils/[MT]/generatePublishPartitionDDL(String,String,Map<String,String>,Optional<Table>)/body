{
  if (!destinationTableMeta.isPresent() || partitionsDMLInfo.size() == 0) {
    return StringUtils.EMPTY;
  }
  StringBuilder ddl=new StringBuilder();
  for (  Map.Entry<String,String> partition : partitionsDMLInfo.entrySet()) {
    ddl.append(String.format("ALTER TABLE %s EXCHANGE PARTITION (%s='%s') WITH TABLE %s;",finalTableName,partition.getKey(),partition.getValue(),stagingTableName)).append("\n");
  }
  return ddl.toString();
}
