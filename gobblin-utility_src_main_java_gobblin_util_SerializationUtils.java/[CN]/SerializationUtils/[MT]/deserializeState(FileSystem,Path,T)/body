{
  Closer closer=Closer.create();
  try {
    InputStream is=closer.register(fs.open(jobStateFilePath));
    DataInputStream dis=closer.register((new DataInputStream(is)));
    state.readFields(dis);
  }
 catch (  Throwable t) {
    throw closer.rethrow(t);
  }
 finally {
    closer.close();
  }
}
