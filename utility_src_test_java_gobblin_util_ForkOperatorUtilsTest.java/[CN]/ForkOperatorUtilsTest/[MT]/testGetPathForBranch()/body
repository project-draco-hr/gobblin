{
  Assert.assertEquals(ForkOperatorUtils.getPathForBranch(PATH_FOO,FORK_BRANCH_NAME_0,0),PATH_FOO);
  Assert.assertEquals(ForkOperatorUtils.getPathForBranch(PATH_FOO,FORK_BRANCH_NAME_0,1),PATH_FOO);
  Assert.assertEquals(ForkOperatorUtils.getPathForBranch(PATH_FOO,FORK_BRANCH_NAME_0,2),PATH_FOO + "/" + FORK_BRANCH_NAME_0);
  Assert.assertEquals(ForkOperatorUtils.getPathForBranch(PATH_FOO,FORK_BRANCH_NAME_1,2),PATH_FOO + "/" + FORK_BRANCH_NAME_1);
}
