{
  this.dataset=builder.dataset;
  this.fs=builder.fs;
  this.state=builder.state;
  this.lateDataThresholdForRecompact=builder.lateDataThresholdForRecompact;
  this.inputDeduplicated=this.state.getPropAsBoolean(MRCompactor.COMPACTION_INPUT_DEDUPLICATED,MRCompactor.DEFAULT_COMPACTION_INPUT_DEDUPLICATED);
  this.outputDeduplicated=this.state.getPropAsBoolean(MRCompactor.COMPACTION_OUTPUT_DEDUPLICATED,MRCompactor.DEFAULT_COMPACTION_OUTPUT_DEDUPLICATED);
  this.recompactFromInputPaths=this.state.getPropAsBoolean(MRCompactor.COMPACTION_RECOMPACT_FROM_INPUT_FOR_LATE_DATA,MRCompactor.DEFAULT_COMPACTION_RECOMPACT_FROM_INPUT_FOR_LATE_DATA);
  this.recompactFromOutputPaths=this.state.getPropAsBoolean(MRCompactor.COMPACTION_RECOMPACT_FROM_DEST_PATHS,MRCompactor.DEFAULT_COMPACTION_RECOMPACT_FROM_DEST_PATHS);
}
