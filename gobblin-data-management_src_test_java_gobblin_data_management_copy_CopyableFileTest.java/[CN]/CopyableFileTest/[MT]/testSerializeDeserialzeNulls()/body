{
  CopyableFile copyableFile=new CopyableFile(null,null,new Path("/relative"),new OwnerAndPermission("owner","group",FsPermission.getDefault()),Lists.newArrayList(new OwnerAndPermission(null,"group2",FsPermission.getDefault())),"checksum".getBytes(),PreserveAttributes.fromMnemonicString(""),"");
  String serialized=CopyableFile.serialize(copyableFile);
  CopyableFile deserialized=CopyableFile.deserialize(serialized);
  Assert.assertEquals(deserialized,copyableFile);
}
