{
  String filePath=getClass().getClassLoader().getResource(resourcePath).getFile();
  FileStatus status=new FileStatus(0l,false,0,0l,0l,new Path(filePath));
  return CopyableFile.builder(FileSystem.getLocal(new Configuration()),status,new Path("/"),CopyConfiguration.builder().targetRoot(new Path("/")).preserve(PreserveAttributes.fromMnemonicString("")).build()).build();
}
