{
  if (!Boolean.valueOf(properties.getProperty(ConfigurationKeys.METRICS_REPORTING_JMX_ENABLED_KEY,ConfigurationKeys.DEFAULT_METRICS_REPORTING_JMX_ENABLED))) {
    LOGGER.info("Not reporting metrics to JMX");
    return;
  }
  this.jmxReporter=Optional.of(JmxReporter.forRegistry(this.metricRegistry).convertRatesTo(TimeUnit.SECONDS).convertDurationsTo(TimeUnit.MILLISECONDS).build());
}
