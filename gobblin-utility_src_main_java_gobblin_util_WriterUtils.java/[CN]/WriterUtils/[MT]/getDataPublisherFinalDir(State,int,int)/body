{
  Preconditions.checkArgument(state.contains(ForkOperatorUtils.getPropertyNameForBranch(ConfigurationKeys.DATA_PUBLISHER_FINAL_DIR,numBranches,branchId)),"Missing required property " + ConfigurationKeys.DATA_PUBLISHER_FINAL_DIR);
  return new Path(state.getProp(ForkOperatorUtils.getPropertyNameForBranch(ConfigurationKeys.DATA_PUBLISHER_FINAL_DIR,numBranches,branchId)),WriterUtils.getWriterFilePath(state,numBranches,branchId));
}
