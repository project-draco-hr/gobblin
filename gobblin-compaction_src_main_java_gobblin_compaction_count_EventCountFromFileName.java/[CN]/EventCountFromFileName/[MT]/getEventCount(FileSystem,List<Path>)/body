{
  long eventCount=0;
  try {
    for (    Path path : paths) {
      String[] parts=FilenameUtils.removeExtension(path.getName()).split("\\.");
      eventCount+=Long.parseLong(parts[parts.length - 1]);
    }
  }
 catch (  Exception e) {
    log.error("Failed to retrieve count from file name");
  }
  return eventCount;
}
