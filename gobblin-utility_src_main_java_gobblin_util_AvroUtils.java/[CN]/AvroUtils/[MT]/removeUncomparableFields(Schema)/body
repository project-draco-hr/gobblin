{
switch (schema.getType()) {
case RECORD:
    return removeUncomparableFieldsFromRecord(schema);
case UNION:
  return removeUncomparableFieldsFromUnion(schema);
case MAP:
return Optional.absent();
case ARRAY:
return Optional.absent();
case ENUM:
return Optional.absent();
default :
return Optional.of(schema);
}
}
